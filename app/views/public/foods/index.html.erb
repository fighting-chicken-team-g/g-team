
    <div class="col-xs-3">
        <%#= render 'shared/genre', genres: @genres %>
    </div>
    <div class="col-xs-9">
        <h2><%= "#{@index}一覧 （全#{@foods_all.count}件）" %></h2>
        <% @foods.each do |food| %>
            <div class="food-box">
                <table class="table">
                    <tr>
                        <th><%= link_to food_path(food) do %>
                            <%= attachment_image_tag(food, :image,size: "180x130", fallback: "no_image.jpg") %>
                            <% end %>
                        </th>
                    </tr>
                    <tr>
                        <th><%= link_to food.name, food_path(food) %></th>
                    </tr>
                    <tr>
                        <th>￥<%= (food.price * 1.1).to_i %></th>
                    </tr>
                </table>
            </div>
        <% end %>
        <%= paginate @foods %>
    </div>
