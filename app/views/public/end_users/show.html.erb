<h3>マイページ</h3>
<table class='table'>
  <thead>
    <tr>
      <th>
        登録情報
        <%= link_to '編集する', edit_end_users_path, class: "btn btn-outline-success" %>
      </th>
    </tr>
    <tr>
      <td>
        氏名：
        <%= current_end_user.last_name %>
        <%= current_end_user.first_name %>
      </td>
    </tr>
    <tr>
      <td>
        カナ：
        <%= current_end_user.last_name_read %>
        <%= current_end_user.first_name_read %>
      </td>
    </tr>
    <tr>
      <td>
        郵便番号：
        <%= current_end_user.post_code %>
      </td>
    </tr>
    <tr>
      <td>
        住所：
        <%= current_end_user.address %>
      </td>
    </tr>
    <tr>
      <td>
        電話番号：
        <%= current_end_user.tel %>
      </td>
    </tr>
    <tr>
      <td>
        メールアドレス：
        <%= current_end_user.email %>
      </td>
    </tr>
